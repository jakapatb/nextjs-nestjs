{
      "compilerOptions": {
            "target": "es2019",
            "lib": [
                  "dom",
                  "dom.iterable",
                  "esnext"
            ],
            "allowJs": true,
            "skipLibCheck": true,
            "strict": false,
            "forceConsistentCasingInFileNames": true,
            "noEmit": true,
            "esModuleInterop": true,
            "module": "esnext",
            "moduleResolution": "node",
            "resolveJsonModule": true,
            "isolatedModules": true,
            "jsx": "preserve",
            "downlevelIteration": true,
            "typeRoots": [
                  "frontend/@types"
            ],
            "baseUrl": "."
      },
      "compileOnSave": true,
      "exclude": [
            "node_modules"
      ],
      "include": [
            "next-env.d.ts",
            "**/**/*.ts",
            "**/*.ts",
            "**/*.tsx",
            "**./*.d.ts"
      ]
}